\begin_layout Plain Layout
\begin_inset Caption Standard

\begin_layout Plain Layout
Categorias de fraturas 
\begin_inset CommandInset label
LatexCommand label
name "fig:Categorias-de-fraturas"

\end_inset


\end_layout

\end_inset


\end_layout

\end_inset


